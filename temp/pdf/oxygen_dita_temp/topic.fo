            <fo:table-cell border-after-width.conditionality="retain" border-bottom-color="black" border-bottom-style="solid" border-bottom-width="1pt" border-right-color="black" border-right-style="solid" border-right-width="1pt" border-top-color="black" border-top-style="solid" border-top-width="1pt" text-align="from-table-column()"><fo:block end-indent="3pt" space-after="3pt" space-after.conditionality="retain" space-before="3pt" space-before.conditionality="retain" start-indent="3pt">Can edit all</fo:block></fo:table-cell>
            <fo:table-cell border-after-width.conditionality="retain" border-bottom-color="black" border-bottom-style="solid" border-bottom-width="1pt" border-right-color="black" border-right-style="solid" border-right-width="1pt" border-top-color="black" border-top-style="solid" border-top-width="1pt" text-align="from-table-column()"><fo:block end-indent="3pt" space-after="3pt" space-after.conditionality="retain" space-before="3pt" space-before.conditionality="retain" start-indent="3pt">Can edit all</fo:block></fo:table-cell>
